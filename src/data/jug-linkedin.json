[
  {
    "command": "open_tab",
    "url": "https://www.linkedin.com/search/results/all/?keywords={{keywords}}"
  },
  {
    "command": "send_event",
    "event": "scroll",
    "data": {
      "scrollDistance": 1000
    }
  },
  {
    "command": "click",
    "selector": "#ember18"
  },
  {
    "command": "highlight",
    "selector": ".t-12.t-black--light.mb2.mh4"
  },
  {
    "command": "scrape",
    "selector": ".t-12.t-black--light.mb2.mh4"
  },
  {
    "command": "screenshot"
  },
  {
    "command": "hitEnter",
    "selector": ".search-global-typeahead__input"
  },
  {
    "command": "wait",
    "time": 2000
  }
]
